// barChart
model barChartprogrambuilder {
  id                String   @id @default(uuid())
  numberOfDataset   Int
  firstFiledDataset Int
  lastFiledDAtaset  Int
  filter_By         String?
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt

  chartTable   ChartTableProgramBuilder @relation(fields: [ChartTableId], references: [id])
  ChartTableId String                   @unique

  widgets WidgetProgramBuilder[]
}

